; MSINFO INF file

[version]
signature="$CHICAGO$"
SetupClass=BASE
LayoutFile=layout.inf

; Base options only happen during Windows Setup
; Setting up the registry settings and INF file locations

[BaseWinOptions]
Msinfo.reg
Msinfo2.reg
online.info.reg

[DefaultInstall]
CopyFiles       =Online.copy.OnlineServices, online.copy.prog.comp, online.copy.prog.AOL, online.copy.prog.att, online.copy.prog.prodigy, icwhack.copy
AddReg          =Online.reg
UpdateInis      =Online.Links

[Msinfo.reg]
CopyFiles       =msinfo.copyfiles.inf, Help.CopyFiles, citCopy.sys, online.copy.win, logo.copy.sys
AddReg          =Online.reg
UpdateInis      =Online.Links

[Msinfo2.reg]
CopyFiles = Online.copy.OnlineServices, online.copy.prog.comp, online.copy.prog.AOL, online.copy.prog.att, online.copy.prog.prodigy, icwhack.copy


[online.info.reg]
;CopyFiles       =online.copy.prog.comp.tip, shock.copy

[destinationdirs]
CitCopy.sys                     =11   ; windows\system
Msinfo.copyfiles.inf            =17   ; windows\inf
Help.CopyFiles                  =18   ; windows\help
Online.copy.sys                 =11
Online.copy.hlp                 =18
Online.copy.prog                =24,%PROGRAMF%\%QCDPROG%
Online.copy.win                 =25
Online.copy.inf                 =17
Online.copy.OnlineServices      =25,%Desktop%\%OnlineShort%
online.copy.prog.comp           =24,%OEM_PROGRAMF%\%OnlineShort%\%CompuShort%
online.copy.prog.comp.tip       =24,%OEM_PROGRAMF%\%OnlineShort%\%CompuShort%
online.copy.prog.AOL            =24,%OEM_PROGRAMF%\%OnlineShort%\%AOL%
online.copy.prog.att            =24,%OEM_PROGRAMF%\%OnlineShort%\%att%
;online.copy.prog.prodigy        =24,%OEM_PROGRAMF%\%OnlineShort%\%Prodigy%
shock.copy                      =24,%OEM_PROGRAMF%\%OEM_MICROS%
logo.copy.sys                   =30
;icwhack.copy                    =24,%OEM_PROGRAMF%\%Internet%\%ICW%

[Msinfo.copyfiles.inf]
msinfo.inf

[online.copy.prog.comp]
cssetup.exe
cs3kit.exe

