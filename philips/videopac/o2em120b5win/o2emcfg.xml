<!-- ********************************************************************************************
 Configuration File for O2EM - don't enter anything in "myConfig" if you don't want to use it. "all" 
 basically serves as a template, just copy the options you want to use to "myConfig" (or create 
 different configs with different names and set the name of the one you want to load in "useConfig" 
 attribute of o2emConfig.
 Any option set in the config file can be overriden on the command line! (e.g. if you set g7400 in 
 here you can always use -o2rom to start with the odyssey2 bios instead of the g7400 bios)
 ******************************************************************************************** -->
<!-- ********************************************************************************************
	Option stack:
		Optionswitch / Default Option
		Game Option
		Command Line Option
******************************************************************************************** -->
<o2emConfig useConfig="myConfig">

	<config name="myConfig">
		<option name="joyautodetect" />
		<option name="browsersortorder">gamename</option>
	</config>

	<config name="c7420" gamerom="videopac/vp_C7420.bin" />

<!-- ********************************************************************************************
 Config that contains all switches for use in your own config(s) - all used in config-tag are 
 used on startup and for browser-config
******************************************************************************************** -->
	<config name="all" gamerom="xxx.rom">

		<!-- disable O2EM internal file browser on startup -->
		<option name="disablebrowser" />
	
		<!-- set sortorder for filebrowser to sort by gamename -->
		<option name="browsersortorder">gamename</option>

		<!-- Set Windowsize to double -->
		<option name="wsize">2</option>
	
		<!-- Use Fullscreen -->
		<option name="fullscreen" />
	
		<!-- Use Scannlines -->
		<option name="scanlines" />
	
		<!-- Autodetect joysticks -->
		<option name="joyautodetect" />
	
		<!-- Turn off sound -->
		<option name="nosound" />
	
		<!-- Turn off voice -->
		<option name="novoice" />
	
		<!-- Set Soundvolume to value -->
		<option name="svolume">100</option>
	
		<!-- Set Voicevolume to value -->
		<option name="vvolume">100</option>
	
		<!-- Start in Debug mode -->
		<option name="debug" />
	
		<!-- Set speed to 100% -->
		<option name="speed">100</option>
	
		<!-- Do not limit speed -->
		<option name="nolimit" />
	
		<!-- Set bios to file -->
		<option name="bios">o2rom.bin</option>
	
		<!-- set rom-directory to value (o2em searches for games there if file is not found in current directory) -->
		<option name="romdir">roms/</option>
	
		<!-- set bios-directory to value (o2em searches for bios there if file is not found in current directory) -->
		<option name="biosdir">bios/</option>
	
		<!-- use Odyssey2 Bios -->
		<option name="o2rom" />
	
		<!-- use C52 Bios -->
		<option name="c52" />
	
		<!-- use G7400 Bios -->
		<option name="g7400" />
	
		<!-- use Jopac Bios -->
		<option name="jopac" />
	
		<!-- use European Timing (50 Hz) -->
		<option name="euro" />

		<!-- force NTSC mode (60 Hz) -->
		<option name="ntsc" />
	
		<!-- use audio low pass filter -->
		<option name="filter" />
	
		<!-- Set screenshot files to dump*.bmp (* will be replaced by numbers) -->
		<option name="scshot">dump@.bmp</option>
	
		<!-- turn on exrom bit for this ROM -->
		<option name="exrom" />
	
		<!-- override system keys with the given values -->
		<option name="s0">ESC,F1,F4,F5,F8,F2,F3,F6,F7,F9,F10,F11</option>
	
		<!-- set joystick 1 to mode 3 (joystick) -->
		<option name="s1">0</option>
	
		<!-- set joystick 2 to mode 3 (joystick) -->
		<option name="s2">0</option>
	
		<!-- enable Emulation of Videopac+ chip -->
		<option name="vppenable" />
	
		<!-- set biosfile to o2rom.bin -->
		<option name="biosfile">bios/o2rom.bin</option>
	
		<!-- force options for a specific ROM on the current game -->
		<option name="crc">A7344D1F</option>
	
		<!-- emulate C7420 Microsoft BASIC cartridge (also needs the correct cartridge as ROM-file for O2EM!) -->
		<option name="c7420" />
	
		<!-- filename for c7420bios (default is c7420_z80.bin in bios directory) -->		
		<option name="c7420bios">bios/c7420_z80.bin</option>
	
		<!-- directory for saving/loading binary files using CLOAD/CSAVE -->
		<option name="c7420dir">basic</option>
	
		<!-- tells CSAVE to save as text instead of binary -->
		<option name="c7420savetext" />
	
		<!-- do not load gamespecific options -->
		<option name="disablegameoption" />
	
		<!-- enable correct disabling of foreground gfx -->
		<option name="useforen" />

		<!-- enable pendirq -->
		<option name="pendirq" />

		<!-- disable horizontal irq -->
		<option name="dishirq" />

		<!-- set region offset -->
		<option name="regionoff">65535</option>

		<!-- set mxsnap -->
		<option name="mxsnap">0</option>
		
		<!-- force special option for evblclk -->
		<option name="evblclk">-1</option>

		<!-- set sprite offset -->
		<option name="sproff">0</option>
		
		<!-- enable tweaked audio -->
		<option name="tweakedaudio" />

		<!-- enable special evbl routine -->
		<option name="handleevbl" />

		<!-- enable special evbll routine -->
		<option name="handleevbll" />

		<!-- enable special draw region routine -->
		<option name="drawregion">0</option>
	
		<!-- switch enter and return in videopac+ mode -->
		<option name="switchenterkeys" />


		<!-- disable joysticks -->
		<option name="s1">0</option>
		<option name="s2">0</option>

	</config>
