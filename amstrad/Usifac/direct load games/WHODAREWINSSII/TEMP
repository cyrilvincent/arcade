NOLIST
ORG #9000
LD A,0
CALL #BC0E
DI
LD A,#C3
LD (#B941),A
LD DE,IRQ
LD HL,#B942
LD (HL),E
INC HL
LD (HL),D
RET

CNT DEFB 0


IRQ DI
PUSH AF:EXX
LD A,16
OUT (C),A
LD A,(CNT)
INC A
LD (CNT),A
RES 7,A
SET 6,A
RES 5,A
OUT (C),A
EXX
POP AF
EX AF,AF'
JP #B945

 (#B941),A
LD DE,IRQ
LD HL,#B942
LD (HL),E
INC HL
LD (HL),D
RET

CNT DEFB 0


IRQ 