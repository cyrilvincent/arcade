; Cybernoid II Loader; Written By Aragorn! 1995ORG  0A000HDILDBC,00000HCALL 0BC38HXOR  ACALL 0BC0EHLDHL,COLOURSLDA,010HLOOP:LD B,(HL)INC HLLD C,BPUSH HLPUSH AFSUB  10HNEGCALL 0BC32HPOP  AFPOP  HLDEC  AJRNZ,LOOPLDHL,0C000H;!..LDDE,0C001H;...LDBC,3FFFH;..?LD(HL),L;uLDIR;..LDHL,LENGTH1LDB,(HL)LDHL,FILE1CALL 0BC77HLDHL,(START1)CALL 0BC83HCALL 0BC7AHCALL 08000HLDHL,LENGTH2LDB,(HL)LDHL,FILE2CALL 0BC77HLDHL,(START2)CALL 0BC83HCALL 0BC7AHLDHL,LENGTH3LDB,(HL)LDHL,FILE3CALL 0BC77HLDHL,(START3)CALL 0BC83HCALL 0BC7AHLDHL,LENGTH4LDB,(HL)LDHL,FILE4CALL 0BC77HLDHL,(START4)CALL 0BC83HCALL 0BC7AHJP0042H;.@.XOR  ALD(02B6BH),A; INF LIVESLD(0162FH),A; INF EXTRASLDA,0C9H; INVULNERABLELD(02AD6H),AJP0042HFILE1:DM "CYBERII.SCR"LENGTH1:DB 0BHSTART1:DW 04000HFILE2:DM "CYBERII.1"LENGTH2:DB 09HSTART2:DW 00042HFILE3:DM "CYBERII.2"LENGTH3:DB 09HSTART3:DW 04100HFILE4:DM "CYBERII.3"LENGTH4:DB 09HSTART4:DW 0C000HCOLOURS:DB 00HDB 1AHDB 06HDB 0AHDB 00HDB 15HDB 01HDB 18HDB 0FHDB 03HDB 10HDB 09HDB 0DHDB 16HDB 02HDB 14HLDBC,3FFFH;..?LD(HL),L;uLDIR;..LDHL,L